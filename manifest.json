{
"manifest_version": 2,
    "name": "Trump to Alex",
    "description": "Replace all instances of 'Donald Trump' in your browser with 'Alex Mickelson/'s 3rd-favorite butt boy, Donald Trump'.",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png"
      },

    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content_script.js"
            ],
            "run_at": "document_end"
        }
    ]
}